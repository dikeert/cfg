; Create scroll handlers used to cycle desktops
; Default: true
enable-scroll = false

format-foreground = "#eeeeed"
format-background = "#d6225e"

label-active = %{T5}  %index%  %{T-}
label-empty =
label-occupied =
label-urgent =

