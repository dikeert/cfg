[module/date]
type = internal/date
interval = 5
#color = #0a6cf5
color = ${colors.foreground}

date = "%a, %b %d"
date-alt = "%a, %b %d"

time = %H:%M
time-alt = %H:%M

format-prefix-foreground = ${self.color}
format-underline = ${self.color}

label = %date% %time%
label-foreground = ${self.color}

; vim:ft=dosini
