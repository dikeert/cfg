Xft.dpi: 120
Xft.hintstyle: hintfull
Xft.autohint: 0
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

XTerm*renderFont: true
XTerm*preeditType: Root
XTerm*xftAntialias: true
XTerm*faceName: Droid Sans Mono
XTerm*faceSize: 10
XTerm*termName: xterm-256color
XTerm*utf8: true
XTerm*utf8Title: true
XTerm*translations: #override \
	Ctrl Alt <KeyPress> 0: set-vt-font(d) \n\
	Ctrl Alt <KeyPress> +: larger-vt-font() \n\
	Ctrl Alt <KeyPress> -: smaller-vt-font() \n\
	Ctrl Shift <KeyPress> C: copy-selection(CLIPBOARD) \n\
	Ctrl Shift <KeyPress> V: insert-selection(CLIPBOARD)

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6

rofi.combi-modi: window,drun
rofi.modi: window,drun
rofi.theme: Arc-Dark
rofi.font: Noto Sans SemiCondensed Medium 14
rofi.drun-icon-theme: Arc
rofi.show-icons: true
rofi.hide-scrollbar: true
